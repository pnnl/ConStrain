@prefix bldg: <http://example.com/mybuilding#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .

# zone 1 and its attached setpoint/sensor
bldg:zone_1 a brick:HVAC_Zone  ;
    brick:hasPoint bldg:zone_1_cooling_temperature_setpoint,
                   bldg:zone_1_heating_temperature_setpoint,
                   bldg:zone_1_zone_air_temperature_sensor,
                   bldg:zone_1_occupancy_sensor .

bldg:zone_1_cooling_temperature_setpoint a brick:Zone_Air_Cooling_Temperature_Setpoint .

bldg:zone_1_heating_temperature_setpoint a brick:Zone_Air_Heating_Temperature_Setpoint .

bldg:zone_1_zone_air_temperature_sensor a brick:Zone_Air_Temperature_Sensor .

bldg:zone_1_occupancy_sensor a brick:Occupancy_Sensor .

# zone 2 and its attached setpoint/sensor
bldg:zone_2 a brick:HVAC_Zone ;
    brick:hasPoint bldg:zone_2_cooling_temperature_setpoint,
                   bldg:zone_2_heating_temperature_setpoint,
                   bldg:zone_2_zone_air_temperature_sensor,
                   bldg:zone_2_occupancy_sensor .

bldg:zone_2_cooling_temperature_setpoint a brick:Zone_Air_Cooling_Temperature_Setpoint .

bldg:zone_2_heating_temperature_setpoint a brick:Zone_Air_Heating_Temperature_Setpoint .

bldg:zone_2_zone_air_temperature_sensor a brick:Zone_Air_Temperature_Sensor .

bldg:zone_2_occupancy_sensor a brick:Occupancy_Sensor .

# service hot water
bldg:service_water_heater a brick:Water_Heater ;
    brick:feeds bldg:service_water_heater_pump ;
    brick:hasPoint bldg:service_water_heater_water_temperature_sensor .

bldg:service_water_heater_pump a brick:Water_Pump ;
    brick:feeds bldg:zone_1 .

bldg:service_water_heater_water_temperature_sensor a brick:Water_Temperature_Sensor .

# AHU
bldg:ahu a brick:AHU  ;
    brick:hasPoint bldg:ahu_air_temperature_setpoint,
                   bldg:ahu_static_pressure_sensor,
                   bldg:ahu_min_static_pressure_setpoint_limit,
                   bldg:ahu_heating_start_stop_status,
                   bldg:ahu_cooling_start_stop_status  ;
    brick:hasPart bldg:ahu_heating_coil,
                  bldg:ahu_cooling_coil,
                  bldg:ahu_fan .

bldg:ahu_air_temperature_setpoint a brick:Air_Temperature_Setpoint .

bldg:ahu_static_pressure_sensor a brick:Static_Pressure_Sensor .

bldg:ahu_min_static_pressure_setpoint_limit a brick:Min_Static_Pressure_Setpoint_Limit .

bldg:ahu_heating_start_stop_status a brick:Heating_Start_Stop_Status .

bldg:ahu_cooling_start_stop_status a brick:Cooling_Start_Stop_Status .

bldg:ahu_heating_coil a brick:Heating_Coil ;
    brick:hasPoint bldg:ahu_heating_coil_on_off_status ;
    brick:hasSubMeter bldg:ahu_heating_coil_electrical_meter .
# 'brick:hasSubMeter' reference: https://docs.brickschema.org/modeling/meters.html#:~:text=%3ABuilding_Electrical_Meter%20.-,Meters%20and%20Submeters,-%23

bldg:ahu_heating_coil_on_off_status a brick:On_Off_Status .

bldg:ahu_heating_coil_electrical_meter a brick:Electrical_Meter .

bldg:ahu_cooling_coil a brick:Cooling_Coil ;
    brick:hasSubMeter bldg:ahu_cooling_coil_thermal_power_meter .
 
bldg:ahu_cooling_coil_thermal_power_meter a brick:Thermal_Power_Meter .

bldg:ahu_fan a brick:Fan ;
    brick:feeds bldg:vav1,
                bldg:vav2 ;
    brick:hasPoint bldg:ahu_fan_on_off_status,
                   bldg:ahu_fan_supply_air_temperature_sensor ;
    brick:hasSubMeter bldg:ahu_fan_electrical_meter .

bldg:ahu_fan_on_off_status a brick:Fan_On_Off_Status .

bldg:ahu_fan_electrical_meter a brick:Electrical_Meter .

bldg:ahu_fan_supply_air_temperature_sensor a brick:Supply_Air_Temperature_Sensor .


# VAV box
bldg:vav1 a brick:Variable_Air_Volume_Box ;
    brick:feeds bldg:zone_1 ;
    brick:haspoint bldg:vav1_air_flow_sensor,
                   bldg:vav1_static_pressure_setpoint,
                   bldg:vav1_air_temperature_setpoint,
                   bldg:vav1_damper_position_sensor .

bldg:vav2 a brick:Variable_Air_Volume_Box ;
    brick:feeds bldg:zone_2 ;
    brick:haspoint bldg:vav2_air_flow_sensor,
                   bldg:vav2_static_pressure_setpoint,
                   bldg:vav2_air_temperature_setpoint,
                   bldg:vav2_damper_position_sensor .

bldg:vav1_air_flow_sensor a brick:Air_Flow_Sensor .

bldg:vav1_static_pressure_setpoint a brick:Static_Pressure_Setpoint .

bldg:vav1_air_temperature_setpoint a brick:Air_Temperature_Setpoint .

bldg:vav1_damper_position_sensor a brick:Damper_Position_Sensor .

bldg:vav2_air_flow_sensor a brick:Air_Flow_Sensor .

bldg:vav2_static_pressure_setpoint a brick:Static_Pressure_Setpoint .

bldg:vav2_air_temperature_setpoint a brick:Air_Temperature_Setpoint .

bldg:vav2_damper_position_sensor a brick:Damper_Position_Sensor .


# chiller and cooling tower
bldg:chiller a brick:Chiller ;
    brick:hasPoint bldg:chiller_chilled_water_flow_sensor,
                   bldg:chiller_chilled_water_return_temperature_sensor ;
    brick:feeds bldg:chilled_water_pump .

bldg:chiller_chilled_water_flow_sensor a brick:Chilled_Water_Flow_Sensor .

bldg:chiller_chilled_water_return_temperature_sensor a brick:Leaving_Water_Temperature_Sensor .

bldg:cooling_tower a brick:Cooling_Tower ;
    brick:hasPoint bldg:cooling_tower_return_condenser_water_temperature_sensor .

bldg:cooling_tower_return_condenser_water_temperature_sensor a brick:Leaving_Water_Temperature_Sensor .

bldg:chilled_water_pump a brick:Chilled_Water_Pump ;
    brick:hasPoint bldg:chilled_water_pump_flow_sensor ;
    brick:feeds bldg:ahu_cooling_coil .

bldg:chilled_water_pump_flow_sensor a brick:Water_Flow_Sensor .


# Boiler
bldg:boiler a brick:Boiler ;
    brick:hasPoint bldg:boiler_hot_water_flow_sensor,
                   bldg:boiler_hot_water_return_temperature_sensor ;
    brick:feeds bldg:hot_water_pump .

bldg:boiler_hot_water_flow_sensor a brick:Hot_Water_Flow_Sensor .

bldg:boiler_hot_water_return_temperature_sensor a brick:Leaving_Hot_Water_Temperature_Sensor .

bldg:boiler_water_flow_sensor a brick:Water_Flow_Sensor .

bldg:hot_water_pump a brick:Hot_Water_Pump ;
    brick:hasPoint bldg:hot_water_pump_water_flow_sensor ;
    brick:feeds bldg:ahu_heating_coil .

bldg:hot_water_pump_water_flow_sensor a brick:Water_Flow_Sensor .


# economizer
bldg:economizer a brick:Economizer ;
    brick:hasPart bldg:outside_damper ;
    brick:hasPoint bldg:economizer_on_off_status .

bldg:outside_damper a brick:Outside_Damper ;
    brick:hasPoint bldg:outside_economizer_outside_air_flow_sensor,
                   bldg:outside_economizer_outside_air_temperature_sensor,
                   bldg:outside_economizer_outside_air_enthalpy_sensor,
                   bldg:outside_economizer_min_outside_air_flow_setpoint_limit .

bldg:economizer_on_off_status a brick:On_Off_Status .

bldg:outside_economizer_outside_air_flow_sensor a brick:Outside_Air_Flow_Sensor .

bldg:outside_economizer_outside_air_temperature_sensor a brick:Outside_Air_Temperature_Sensor .

bldg:outside_economizer_outside_air_enthalpy_sensor a brick:Outside_Air_Enthalpy_Sensor.

bldg:outside_economizer_min_outside_air_flow_setpoint_limit a brick:Min_Outside_Air_Flow_Setpoint_Limit . 